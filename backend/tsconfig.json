{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": ["es6"],
    "rootDir": "./src/",
    "outDir": "./dist/",
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}
